{
  "name": "@realtimex/email-automator",
  "version": "2.8.2",
  "type": "module",
  "main": "dist/api/server.js",
  "bin": {
    "email-automator": "./bin/email-automator.js",
    "email-automator-setup": "./bin/email-automator-setup.js",
    "email-automator-deploy": "./bin/email-automator-deploy.js"
  },
  "files": [
    "bin",
    "api",
    "dist",
    "supabase",
    "scripts",
    "public",
    ".env.example",
    "tsconfig.json",
    "vite.config.ts",
    "tailwind.config.ts",
    "postcss.config.js",
    "index.html",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch api/server.ts",
    "build": "npm run build:ui && npm run build:api",
    "build:ui": "vite build",
    "build:api": "tsc -p tsconfig.api.json",
    "preview": "vite preview",
    "serve": "node dist/api/server.js",
    "start": "node dist/api/server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src api --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "email",
    "automation",
    "ai",
    "gmail",
    "outlook",
    "realtimex",
    "local-app",
    "supabase",
    "edge-functions"
  ],
  "author": "RealTimeX Team",
  "license": "MIT",
  "description": "AI-powered email automation for Gmail and Outlook. Hybrid architecture with Supabase Edge Functions and local AI processing.",
  "repository": {
    "type": "git",
    "url": "https://github.com/therealtimex/email-automator.git"
  },
  "bugs": {
    "url": "https://github.com/therealtimex/email-automator/issues"
  },
  "homepage": "https://github.com/therealtimex/email-automator#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.5",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.90.1",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "googleapis": "^126.0.1",
    "lucide-react": "^0.562.0",
    "mailparser": "^3.9.1",
    "next-themes": "^0.4.6",
    "openai": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.23",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "supabase": "^2.72.7",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
